{
  "expo": {
    "name": "Curently",
    "scheme": "Curently",
    "slug": "curently",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kiran07x.curently"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Audiowide-Regular.ttf",
            "./assets/fonts/Montserrat-VariableFont_wght.ttf",
            "./assets/fonts/Montserrat-Italic-VariableFont_wght.ttf",
            "./assets/fonts/OpenSans-VariableFont_wdth,wght.ttf",
            "./assets/fonts/OpenSans-Italic-VariableFont_wdth,wght.ttf",
            "./assets/fonts/Montserrat-Regular.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "aaa3626b-f36a-45a9-95d9-c1d2029e7e5b"
      }
    },
    "owner": "kiran07x"
  }
}
